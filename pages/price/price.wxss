.page {
  width: 100%;
  height: 100%;
  background-color: #fff;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 15px;
  position: fixed;
  display: inline-block;
}
.scroll_left {
    width: 30%;
    height: 100%;
    display: inline-block;
    background-color: #F6F6F6;
}
.scroll_right{
    width: 70%;
    height: 100%;
    display: inline-block;
    background-color: #fff;
}
.section {
    background-color: #F6F6F6;
    height: 60rpx;
    padding-left: 15rpx;
    line-height: 60rpx;
    color: #cccccc;
    margin-left: 30rpx;
}
.abc {
    margin-top: 20rpx;
}
.section_head {
    width: 100%;
    height: 90rpx;
    margin-left: 30rpx;
    display: inline-block;
}
.section_head_title {
    width: 85%;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    display: inline-block;
}
.section_head_img {
    display: inline-block;
    width: 32rpx;
    height: 32rpx;
}
.left_item {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-right: 1px solid #DCDCDC;
    border-bottom: 1px solid #DCDCDC;
}
.left_item_first {
    border-top: 1px solid #DCDCDC;
}
.left_item_select {
    background-color: #fff;
    border-right-width: 0;
    color: #1794F7;
}
.right_item {
    height: 95rpx;
    width: 100%;
    display: inline-block;
    
}
.right_item_image {
    width: 85rpx;
    height: 45rpx;
    align-items: center;
    display: inline-block;
    margin-left: 60rpx;
    margin-top: 30rpx;
}
.right_item_title {
    height: 95rpx;
    width: 60%;
    line-height: 95rpx;
    margin-left: 60rpx;
    display: inline-block;
    border-bottom: 1px solid #DCDCDC;
}
.nonet {
    width: 85%;
    margin-left: 30rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-bottom: 1px solid #F6F6F6;
}